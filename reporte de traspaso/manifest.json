{
  "manifest_version": 3,
  "name": "Reporte de traspaso",
  "description": "Reporte para controlar traspasos",
  "version": "0.0.1",
  "icons":{
    "128": "/report.png"
  },
  "content_scripts":[
    {
      "matches":["<all_urls>"],
      "js":["popup.js"]
    }
  ],
  "background": {
    "service_workers": ["background.js"]
  },
  "permissions": [
    "tabs"
  ]
,
  "action": {
  "default_popup": "popup.html"
}
}